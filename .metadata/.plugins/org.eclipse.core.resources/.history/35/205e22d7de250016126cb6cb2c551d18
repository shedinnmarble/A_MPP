
public class Role {
	
	private  RoleType roleType;
	public enum RoleType{
		Developer,
		Tester,
		/**
		 * project manager
		 */
		ProjectManager,
		ScrumMaster				
	}
	public RoleType getRoleType() {
		return roleType;
	}
	public void setRoleType(RoleType roleType) {
		this.roleType = roleType;
	};
	  
	
	
	
}
